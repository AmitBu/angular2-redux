{
  "name": "angular2-and-redux",
  "version": "4.0.0",
  "description": "based on Angular webpack starter by AngularClass",
  "author": "Patrick Stapleton <patrick@angularclass.com>",
  "homepage": "https://github.com/angularclass/angular2-webpack-starter",
  "license": "MIT",
  "scripts": {
    "server": "npm run server:dev",
    "server:dev": "node --max-old-space-size=4096 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "start": "npm run server",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.11",
    "core-js": "2.1.5",
    "ng2-redux": "2.0.2",
    "redux": "3.3.1",
    "rxjs": "5.0.0-beta.2",
    "zone.js": "0.6.4"
  },
  "devDependencies": {
    "angular2-hmr": "0.2.2",
    "awesome-typescript-loader": "0.16.2",
    "compression-webpack-plugin": "0.3.0",
    "copy-webpack-plugin": "1.1.1",
    "css-loader": "0.23.1",
    "es6-promise": "3.1.2",
    "es6-promise-loader": "1.0.1",
    "es6-shim": "0.35.0",
    "es7-reflect-metadata": "1.6.0",
    "exports-loader": "0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.9.0",
    "http-server": "0.9.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "minimatch": "3.0.0",
    "ng2lint": "0.0.10",
    "node-sass": "^3.4.2",
    "parse5": "1.3.2",
    "postcss-loader": "^0.8.2",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "2.5.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.0",
    "ts-helper": "0.0.1",
    "ts-node": "0.7.0",
    "tslint": "3.6.0",
    "tslint-loader": "2.1.3",
    "typedoc": "0.3.12",
    "typescript": "1.8.9",
    "typings": "0.7.9",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-md5-hash": "0.0.5"
